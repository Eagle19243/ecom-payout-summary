{
    "name": "Ecom Payout Summary",
    "version": "1.0.0",
    "description": "This chrome extension works to make a report to google sheet from csv",
    "author": "Eagle19243",
    "manifest_version": 2,
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "options_page": "html/options.html",
    "page_action": {
        "default_icon": {
            "16": "images/icon-16.png",
            "32": "images/icon-32.png",
            "48": "images/icon-48.png"
        }
    },
    "background": {
        "page": "html/background.html",
        "persistent": true
    },
    "permissions": [
        "storage",
        "identity"
    ],
    "content_security_policy": "script-src 'self' https://apis.google.com/; object-src 'self'",
    "oauth2": {
        "client_id": "567918662206-pnmtg2fq09nh212kfatd06mc5307g44t.apps.googleusercontent.com",
        "scopes":["https://www.googleapis.com/auth/spreadsheets"]
    },
    "key": ""
}
  